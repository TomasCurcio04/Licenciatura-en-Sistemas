var
	num1,num2,suma:real;
begin
	writeln('ingrese dos numeros por teclado');
	readln(num1);
	readln(num2);
	suma:=num1+num2;
	writeln('El resultado de la suma de los numeros con la opcion a es:', suma:2:2);
	writeln('El resultado de la suma de los numeros con la opcion b es:', num1 + num2:2:2);
end.
