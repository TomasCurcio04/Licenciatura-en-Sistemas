#!/bin/bash
echo "Ingrese dos numeros"
read num1 num2
echo "El resultado de la multiplicacion de estos numeros es: $((num1 * num2))"
echo "El resultado de la suma de estos numeros es: $((num1 + num2))"
echo "El resultado de la resta de estos numeros es: $((num1 - num2))"

if [ $num1 -gt $num2 ]; then
	echo "El mayor numero leido es: $num1 "
else
	echo "El mayor numero leido es: $num2"
fi
